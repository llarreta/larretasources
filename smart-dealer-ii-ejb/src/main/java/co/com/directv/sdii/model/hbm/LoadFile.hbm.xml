<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="co.com.directv.sdii.model.pojo.LoadFile" table="LOAD_FILES">
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="38" scale="0" />
            <generator class="sequence">
                <param name="sequence">SEQ_LOAD_FILE</param>
            </generator>
        </id>
        <many-to-one name="loadFileType" class="co.com.directv.sdii.model.pojo.LoadFileType" fetch="select" lazy="false">
            <column name="LOAD_FILE_TYPE_ID" precision="38" scale="0" not-null="true" />
        </many-to-one>
        <many-to-one name="uploadFile" class="co.com.directv.sdii.model.pojo.UploadFile" fetch="select" lazy="false">
            <column name="UPLOAD_FILE_ID" precision="38" scale="0" not-null="true" />
        </many-to-one>
        <property name="objectFile" type="blob">
            <column name="OBJECT_FILE" not-null="true" />
        </property>
        <property name="objectName" type="java.lang.String">
            <column name="OBJECT_NAME" length="1000" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
