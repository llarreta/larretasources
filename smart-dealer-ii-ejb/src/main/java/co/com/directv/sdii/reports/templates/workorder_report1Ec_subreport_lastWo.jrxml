	<subDataset name="Table Dataset 1"/>
	<subDataset name="dataset1"/>
	<parameter name="WORK_ORDER_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[select * from
(
select
WOSH.DESCRIPTION as LAWO_TECHNICIAN_COMMENTS,
S.SERVICE_NAME as LAWO_SERVICE_NAME,
S.SERVICE_CODE as LAWO_SERVICE_CODE
from

wo_status_history wosh,

work_orders wo,
Work_Order_Services wos,
services s
where
WO.CREATION_DATE <
(
select wodd.CREATION_DATE
from work_orders wodd
where wodd.ID = $P{WORK_ORDER_ID}
)
and
WO.CUSTOMER_ID =
(
select WOCU.CUSTOMER_ID
from
work_orders wocu
where
WOCU.ID = $P{WORK_ORDER_ID}
)
and
WOS.WO_ID = WO.ID and
WOS.SERVICE_ID = S.ID and
WOSH.WO_ID = WO.ID and
WOSH.WO_STATUS_ID =
(
select
WOST.ID
from
workorder_status wost
where
WOST.WO_STATE_CODE = 'T'
)
)
where rownum < 2]]>
	</queryString>
