	<queryString>
		<![CDATA[select c.first_name || ' ' || c.last_name as nombreSuscriptor,
c.document_number as idCliente,
woa.contact_person as contPerson,
woa.description as woaDesc,
to_char(wo.wo_programming_date,'DD') as dia,
to_char(wo.wo_programming_date,'MM') as mes,
to_char(wo.wo_programming_date,'YY') as ano,
wo.wo_address as direccionInstalacion,
ct.id as ciudad,
stt.state_name as estado
from
customer c,
work_orders wo,
wo_assignments woas,
work_order_agenda woa,
postal_codes pc,
cities ct,
states stt,

work_order_services wos,
services s,
service_category sc,
service_types st
where
WOAS.WO_ID = WO.ID and
WOA.WO_ASSIGNMENTS_ID = WOAS.IS_ACTIVE and
C.ID = WO.CUSTOMER_ID and
WOS.WO_ID = WO.ID and
WOS.SERVICE_ID = S.ID and
S.SERVICE_CATEGORY_ID = SC.ID and
SC.SERVICE_TYPE_ID = ST.ID and
WO.CUSTOMER_ID = C.ID and
WOA.STATUS = 'S' and
C.POSTAL_CODE_ID = PC.ID and
PC.CITY_ID = CT.ID and
CT.STATE_ID = STT.ID and
WO.ID = 600450;]]>
