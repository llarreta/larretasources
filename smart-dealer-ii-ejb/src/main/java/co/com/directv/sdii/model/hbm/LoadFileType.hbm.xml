<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="co.com.directv.sdii.model.pojo.LoadFileType" table="LOAD_FILE_TYPES">
        <id name="id" type="java.lang.Long">
            <column name="ID" precision="38" scale="0" />
            <generator class="sequence">
                <param name="sequence">SEQ_LOAD_FILE_TYPE</param>
            </generator>
        </id>
        <property name="code" type="java.lang.String">
            <column name="CODE" length="10" not-null="true" />
        </property>
        <property name="description" type="java.lang.String">
            <column name="DESCRIPTION" length="1000" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
