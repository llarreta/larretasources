	<parameter name="WORK_ORDER_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT count(1) as cantAsist
from work_orders wo
where
WO.CUSTOMER_ID =
(select WOCU.CUSTOMER_ID
from
work_orders wocu
where
WOCU.ID = $P{WORK_ORDER_ID}
)
and
WO.CREATION_DATE >=
(select (wodd.CREATION_DATE-90)
from work_orders wodd
where wodd.ID = $P{WORK_ORDER_ID}
)]]>
	</queryString>
	<field name="CANTASIST" class="java.math.BigDecimal"/>
	<background>
		<band splitType="Stretch"/>
	</background>
