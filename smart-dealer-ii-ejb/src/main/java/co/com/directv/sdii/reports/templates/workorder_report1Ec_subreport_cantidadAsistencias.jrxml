<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="workorder_report1Ec_subreport_cantidadAsistencias" language="groovy" pageWidth="29" pageHeight="29" orientation="Landscape" whenNoDataType="AllSectionsNoDetail" columnWidth="29" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" whenResourceMissingType="Empty">
	<property name="ireport.zoom" value="1.7715610000000015"/>
	<property name="ireport.x" value="0"/>
