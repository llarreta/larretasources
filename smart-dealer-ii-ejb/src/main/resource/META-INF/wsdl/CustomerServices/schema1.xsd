      <xsd:element minOccurs="0" name="serviceCategoryName" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ServiceType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="serviceTypeCode" type="xsd:string"/>
      <xsd:element minOccurs="0" name="serviceTypeName" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ServiceStatus">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="serviceStatusCode" type="xsd:string"/>
      <xsd:element minOccurs="0" name="serviceStatusName" type="xsd:string"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ServiceCollection">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" minOccurs="0" name="Service" type="Service"/>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
