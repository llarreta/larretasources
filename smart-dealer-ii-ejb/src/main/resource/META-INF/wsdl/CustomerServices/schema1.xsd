<xsd:schema xmlns="http://directvla.com/schema/businessdomain/service/v1-0" xmlns:commonbd-v1-0="http://directvla.com/schema/businessdomain/common/v1-0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://directvla.com/schema/businessdomain/service/v1-0">
  <xsd:import namespace="http://directvla.com/schema/businessdomain/common/v1-0" schemaLocation="schema2.xsd"/>
  <xsd:complexType name="Service">
    <xsd:sequence>
      <xsd:element name="serviceKey" type="xsd:string"/>
      <xsd:element minOccurs="0" name="name" type="xsd:string"/>
      <xsd:element minOccurs="0" name="code" type="xsd:string"/>
      <xsd:element minOccurs="0" name="description" type="xsd:string"/>
      <xsd:element minOccurs="0" name="productSerialNumber" type="xsd:string"/>
      <xsd:element minOccurs="0" name="validFor" type="commonbd-v1-0:TimePeriod"/>
      <xsd:element minOccurs="0" name="isShippingOrderRequired" type="xsd:string"/>
      <xsd:element minOccurs="0" name="serviceCategory" type="ServiceCategory"/>
