          <xsd:element minOccurs="0" name="nationality" type="xsd:anySimpleType"/>
          <xsd:element minOccurs="0" name="maritalStatus" type="xsd:anySimpleType"/>
          <xsd:element minOccurs="0" name="skills" type="xsd:anySimpleType"/>
