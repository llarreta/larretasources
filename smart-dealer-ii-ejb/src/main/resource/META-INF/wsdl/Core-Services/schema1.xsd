<xsd:schema xmlns:crm-common="http://directvla.com/schema/crm/common/v1-1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://directvla.com/schema/crm/common/v1-1">
