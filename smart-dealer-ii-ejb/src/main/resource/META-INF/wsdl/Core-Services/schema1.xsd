      <xsd:extension base="crm-common:PartyIdentification">
        <xsd:sequence>
          <xsd:element maxOccurs="unbounded" minOccurs="0" name="Individual" type="crm-common:Individual"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
