          <xsd:element maxOccurs="unbounded" minOccurs="0" name="IndividualIdentification" type="crm-common:IndividualIdentification"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
