          <xsd:element minOccurs="0" name="dealerISalesd" type="xsd:long"/>
          <xsd:element minOccurs="0" name="facilityCode" type="xsd:long"/>
          <xsd:element minOccurs="0" name="action" type="xsd:string"/>
