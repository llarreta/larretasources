    <xsd:sequence>
      <xsd:element name="requestID" type="crm-common:RequestIDType"/>
      <xsd:element name="sourceID" type="crm-common:SourceIDType"/>
      <xsd:element minOccurs="0" name="systemID" type="crm-common:SystemIDType"/>
      <xsd:element minOccurs="0" name="appID" type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="validate" type="xsd:boolean"/>
      <xsd:element minOccurs="0" name="debug" type="xsd:boolean"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ResponseMetadataType">
    <xsd:sequence>
      <xsd:element name="requestID" type="crm-common:RequestIDType"/>
      <xsd:element name="timeStamp" type="crm-common:TimeStampType"/>
      <xsd:element minOccurs="0" name="boxUsage" type="crm-common:BoxUsageType"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="RequestIDType">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="SourceIDType">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="SystemIDType">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="AppIDType">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="TimeStampType">
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>
  <xsd:simpleType name="BoxUsageType">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="CodeType">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:simpleType name="MessageType">
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>
  <xsd:complexType name="TimePeriod">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="startDateTime" type="xsd:dateTime"/>
      <xsd:element minOccurs="0" name="endDateTime" type="xsd:dateTime"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType abstract="true" name="ContactMedium">
    <xsd:sequence>
