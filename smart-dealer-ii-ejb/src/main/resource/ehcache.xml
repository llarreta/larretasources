<?xml version="1.0" encoding="UTF-8"?>
<ehcache>

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
        maxElementsInMemory="10000"
        eternal="false"
        timeToIdleSeconds="3600"
        timeToLiveSeconds="3600"
        overflowToDisk="true"
        diskPersistent="false"
        diskExpiryThreadIntervalSeconds="120" 
    />    
        
        
	<cache
		name="org.hibernate.cache.StandardQueryCache"
		maxElementsInMemory="10000"
		eternal="false"
		timeToLiveSeconds="3600"
		overflowToDisk="true"
	/>
        
	<cache
		name="co.com.directv.sdii.model.pojo.Rol"
		maxElementsInMemory="10000"
		eternal="false"
		timeToIdleSeconds="3600"
		timeToLiveSeconds="3600"
		overflowToDisk="true"
	/>

	<cache
		name="co.com.directv.sdii.model.pojo.User"
		maxElementsInMemory="10000"
		eternal="false"
		timeToIdleSeconds="3600"
		timeToLiveSeconds="3600"
		overflowToDisk="true"
	/>

	<cache
		name="co.com.directv.sdii.model.pojo.SdiiTimeZone"
		maxElementsInMemory="10000"
		eternal="false"
		timeToIdleSeconds="3600"
		timeToLiveSeconds="3600"
		overflowToDisk="true"
	/>

</ehcache>